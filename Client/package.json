{
  "name": "Weather-App",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@types/dotenv": "^6.1.0",
    "@types/google-protobuf": "^3.2.7",
    "@types/react-bootstrap": "^0.32.15",
    "@types/superagent": "^3.8.6",
    "foreman": "^3.0.1",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-scripts-ts": "3.1.0",
    "superagent": "^4.1.0"
  },
  "homepage": "./",
  "main": "src/electron-starter.js",
  "scripts": {
    "start": "react-scripts-ts start",
    "build": "react-scripts-ts build",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject",
    "electron": "node_modules/.bin/electron .",
    "dev": "nf start -p 3000",
    "package:mac": "react-scripts-ts build && electron-builder --mac --x64",
    "package:windows": "react-scripts-ts build && electron-builder --win --x64",
    "package:linux": "react-scripts-ts build && electron-builder --linux --x64"
  },
  "devDependencies": {
    "@types/jest": "^23.3.12",
    "@types/node": "^10.12.18",
    "@types/react": "^16.7.18",
    "@types/react-dom": "^16.0.11",
    "antd": "^3.12.1",
    "dotenv": "^6.2.0",
    "electron": "^4.0.1",
    "electron-builder": "^20.38.5",
    "grpc-tools": "^1.6.6",
    "grpc-web": "^1.0.3",
    "grpc_tools_node_protoc_ts": "^2.4.2",
    "react-bootstrap": "^0.32.4",
    "typescript": "^3.2.2"
  },
  "build": {
    "productName": "Weather-App",
    "appId": "try.example.com",
    "files": [
      "./src/electron-starter.js",
      "./icon/**/*",
      "*.js",
      "build",
      "node_modules"
    ],
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "icon": "./icon/favicon.icns",
      "target": [
        "dmg"
      ]
    },
    "win": {
      "icon": "./icon/favicon.ico",
      "target": "msi"
    }
  },
  "description": "お天気アプリ",
  "repository": "https://github.com/butachin/electron-weather-app.git",
  "author": "butachin",
  "license": "MIT"
}
